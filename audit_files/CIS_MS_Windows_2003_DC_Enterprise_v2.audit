    acl_inheritance	: "not used"
    acl_apply		: "This key and subkeys"
    acl_allow		: "Read"
   </user>
  </acl>

  # 4.4.2.2 HKLM\Software\Microsoft\Windows\CurrentVersion\Installer: Administrators: Full; System: Full; Users: Read

  <custom_item>
   type			: REGISTRY_PERMISSIONS
   description	: "4.4.2.2 HKLM\Software\Microsoft\Windows\CurrentVersion\Installer"
   info			: "The registry contains many sensitive settings that control the behavior of almost all aspects of the system and its corresponding applications. The operating system allows specific access controls to be placed on each registry folder, similar to permissions on file folders. Administrators and the System have Full Control, Users have Read permissions only."
   solution		: "Make sure permissions for the registry location HKLM\Software\Microsoft\Windows\CurrentVersion\Installer is set to full for Administrators and System and set to read only for Users."
   see_also		: "https://benchmarks.cisecurity.org/tools2/windows/CIS_Win2003_DC_Benchmark_v2.0.pdf"
   value_type	: REG_ACL
   reg_key		: "HKLM\Software\Microsoft\Windows\CurrentVersion\Installer"
   value_data	: "Admin System and Users"
  </item>

