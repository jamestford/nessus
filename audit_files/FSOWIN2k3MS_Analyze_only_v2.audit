#  [Registry Keys] settings
#--------------------------------------#


<registry_acl: "1">

<user: "Everyone">
	acl_inheritance: "not inherited"
	acl_apply: "This Key and Subkeys"
 	acl_deny: "full control" 
	</user>

</acl>

<custom_item>
