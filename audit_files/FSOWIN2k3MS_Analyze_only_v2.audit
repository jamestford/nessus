#--------------------------------------#
#  [Unicode] settings
#--------------------------------------#

#Unicode=yes

#--------------------------------------#
#  [System Access] settings
#--------------------------------------#


<item>
	name: "Minimum password age"
	value: "[1..60]"
</item>

<item>
	name: "Maximum password age"
	value: "[MIN..60]"
</item>

<item>
	name: "Minimum password length"
	value: "[14..MAX]"
</item>

<item>
	name: "Password must meet complexity requirements"
	value: "enabled"
</item>

<item>
	name: "Enforce password history"
	value: "[24..MAX]"
</item>

<item>
	name: "Account lockout threshold"
	value: "[MIN..3]"
</item>

<item>
	name: "Reset lockout account counter after"
	value: "[60..MAX]"
</item>

<item>
	name: "Account lockout duration"
	value: "0"
</item>

<item>
	name: "Network security: Force logoff when logon hours expire"
	value: "enabled"
</item>

<item>
	name: "Store password using reversible encryption for all users in the domain"
	value: "disabled"
</item>

<item>
	name: "Network access: Allow anonymous SID/Name translation"
	value: "disabled"
</item>

<item>
	name: "Accounts: Guest account status"
	value: "disabled"
</item>

#--------------------------------------#
#  [System Log] settings
#--------------------------------------#


<item>
	name: "Maximum system log size"
	value: "[MIN..16384]"
</item>

<item>
	name: "Retention method for system log"
	value: "Manually"
</item>

<item>
	name: "Prevent local guests group from accessing system log"
