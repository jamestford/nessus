  # 4.4.2 Registry Permissions
  # * Unless stated otherwise, Administrators or System Full Control is full control for the designated key and all subkeys. Creator Owner Full
  # Control is for subkeys only. Users permissions are for current key, subkeys, and values.

  <registry_acl	: "Admin System Creator and Users">
   <user	: "Administrators">
    acl_inheritance	: "not used"
    acl_apply		: "This key and subkeys"
    acl_allow		: "Full Control"
   </user>

   <user	: "SYSTEM">
    acl_inheritance	: "not used"
    acl_apply		: "This key and subkeys"
    acl_allow		: "Full Control"
   </user>

   <user	: "CREATOR OWNER">
    acl_inheritance	: "not used"
    acl_apply		: "Subkeys only"
    acl_allow		: "Full Control"
   </user>

   <user	: "Users">
    acl_inheritance	: "not used"
    acl_apply		: "This key and subkeys"
    acl_allow		: "Read"
   </user>
  </acl>

  <registry_acl	: "Admin System and Users">
   <user	: "Administrators">
    acl_inheritance	: "not used"
    acl_apply		: "This key and subkeys"
    acl_allow		: "Full Control"
   </user>

   <user	: "SYSTEM">
    acl_inheritance	: "not used"
    acl_apply		: "This key and subkeys"
    acl_allow		: "Full Control"
   </user>

   <user	: "Users">
    acl_inheritance	: "not used"
    acl_apply		: "This key and subkeys"
    acl_allow		: "Read"
   </user>
  </acl>

  <registry_acl	: "Admin System and Auth Users">
   <user	: "Administrators">
    acl_inheritance	: "not used"
    acl_apply		: "This key and subkeys"
    acl_allow		: "Full Control"
   </user>

   <user	: "SYSTEM">
    acl_inheritance	: "not used"
    acl_apply		: "This key and subkeys"
    acl_allow		: "Full Control"
   </user>

   <user	: "Authenticated Users">
    acl_inheritance	: "not used"
    acl_apply		: "This key and subkeys"
    acl_allow		: "Read"
   </user>

  </acl>

  <registry_acl	: "Admin System and Creator">
   <user	: "Administrators">
    acl_inheritance	: "not used"
    acl_apply		: "This key and subkeys"
    acl_allow		: "Full Control"
   </user>

   <user	: "SYSTEM">
    acl_inheritance	: "not used"
    acl_apply		: "This key and subkeys"
    acl_allow		: "Full Control"
   </user>

   <user	: "CREATOR OWNER">
    acl_inheritance	: "not used"
    acl_apply		: "Subkeys only"
    acl_allow		: "Full Control"
   </user>
  </acl>

  <registry_acl	: "Admin and Users">
   <user	: "Administrators">
    acl_inheritance	: "not used"
    acl_apply		: "This key and subkeys"
    acl_allow		: "Full Control"
   </user>

   <user	: "Users">
    acl_inheritance	: "not used"
    acl_apply		: "This key and subkeys"
    acl_allow		: "Read"
   </user>
  </acl>

  <registry_acl	: "Admin System Network Service Users">
   <user	: "Administrators">
    acl_inheritance	: "not used"
    acl_apply		: "This key and subkeys"
    acl_allow		: "Full Control"
   </user>

   <user	: "System">
    acl_inheritance	: "not used"
    acl_apply		: "This key and subkeys"
    acl_allow		: "Full Control"
   </user>

