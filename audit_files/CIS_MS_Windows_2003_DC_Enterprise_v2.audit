    acl_apply		: "This folder, subfolders and files"
    acl_allow		: "Full Control"
   </user>

   <user	: "System">
    acl_inheritance	: "not used"
    acl_apply		: "This folder, subfolders and files"
    acl_allow		: "Full Control"
   </user>

   <user	: "Creator Owner">
    acl_inheritance	: "not used"
    acl_apply		: "This folder, subfolders and files"
    acl_allow		: "Full Control"
   </user>

   <user	: "Interactive">
    acl_inheritance	: "not used"
    acl_apply		: "This folder, subfolders and files"
    acl_allow		: "Read & Execute"
   </user>
  </acl>

  <file_acl	: "Admin and System Full">
   <user	: "Administrators">
    acl_inheritance	: "not used"
